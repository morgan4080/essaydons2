{
  "builds": [
    {
      "src": "nuxt.config.js",
      "use": "@nuxtjs/vercel-builder",
      "config": {
        "serverFiles": ["serverMiddleware/**", "_JWTKeys/**"]
      }
    },
    {
      "src": "pages/api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "env": {
    "DATABASE_URL": "@database_url"
  },
  "build": {
    "env": {
      "DATABASE_URL": "@database_url"
    }
  },
  "routes": [
    {
      "src": "pages/api/accounts/(.*)",
      "dest": "pages/api/accounts/$1.js",
      "methods": ["GET", "POST", "PUT", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Methods": "GET, POST, PUT, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Cookie"
      }
    },
    {
      "src": "pages/api/categories/(.*)",
      "dest": "pages/api/categories/$1.js",
      "methods": ["GET", "POST", "PUT", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Methods": "GET, POST, PUT, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Cookie"
      }
    },
    {
      "src": "pages/api/orders/(.*)",
      "dest": "pages/api/orders/$1.js",
      "methods": ["GET", "POST", "PUT", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Methods": "GET, POST, PUT, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Cookie"
      }
    },
    {
      "src": "pages/api/posts/(.*)",
      "dest": "pages/api/posts/$1.js",
      "methods": ["GET", "POST", "PUT", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Methods": "GET, POST, PUT, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Cookie"
      }
    },
    {
      "src": "pages/api/storedata/(.*)",
      "dest": "pages/api/storedata/$1.js",
      "methods": ["GET", "POST", "PUT", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Methods": "GET, POST, PUT, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Cookie"
      }
    },
    {
      "src": "pages/api/stripeTransactions00/(.*)",
      "dest": "pages/api/stripeTransactions00/$1.js",
      "methods": ["GET", "POST", "PUT", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Methods": "GET, POST, PUT, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Cookie"
      }
    },
    {
      "src": "pages/api/users/(.*)",
      "dest": "pages/api/users/$1.js",
      "methods": ["GET", "POST", "PUT", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Methods": "GET, POST, PUT, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Cookie"
      }
    },
    {
      "src": "pages/api/login/(.*)",
      "dest": "pages/api/login/$1.js",
      "methods": ["POST"],
      "headers": {
        "Access-Control-Allow-Methods": "POST",
        "Access-Control-Allow-Headers": "Content-Type, Cookie"
      }
    },
    {
      "src": "pages/api/me/(.*)",
      "dest": "pages/api/me/$1.js",
      "methods": ["GET"],
      "headers": {
        "Access-Control-Allow-Methods": "GET",
        "Access-Control-Allow-Headers": "Content-Type, Cookie"
      }
    }
  ]
}
