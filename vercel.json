{
  "builds": [
    {
      "src": "nuxt.config.js",
      "use": "@nuxtjs/vercel-builder",
      "config": {
        "serverFiles": ["serverMiddleware/**"]
      }
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "env": {
    "MY_VARIABLE": ""
  },
  "build": {
    "env": {
      "MY_VARIABLE": ""
    }
  },
  "routes": [
    {
      "src": "/api/accounts/(.*)",
      "dest": "/api/accounts/$1.js",
      "methods": ["GET", "POST", "PUT", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Methods": "GET, POST, PUT, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type"
      }
    },
    {
      "src": "/api/categories/(.*)",
      "dest": "/api/categories/$1.js",
      "methods": ["GET", "POST", "PUT", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Methods": "GET, POST, PUT, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type"
      }
    },
    {
      "src": "/api/orders/(.*)",
      "dest": "/api/orders/$1.js",
      "methods": ["GET", "POST", "PUT", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Methods": "GET, POST, PUT, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type"
      }
    },
    {
      "src": "/api/posts/(.*)",
      "dest": "/api/posts/$1.js",
      "methods": ["GET", "POST", "PUT", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Methods": "GET, POST, PUT, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type"
      }
    },
    {
      "src": "/api/storedata/(.*)",
      "dest": "/api/storedata/$1.js",
      "methods": ["GET", "POST", "PUT", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Methods": "GET, POST, PUT, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type"
      }
    },
    {
      "src": "/api/stripeTransactions00/(.*)",
      "dest": "/api/stripeTransactions00/$1.js",
      "methods": ["GET", "POST", "PUT", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Methods": "GET, POST, PUT, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type"
      }
    },
    {
      "src": "/api/users/(.*)",
      "dest": "/api/users/$1.js",
      "methods": ["GET", "POST", "PUT", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Methods": "GET, POST, PUT, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type"
      }
    }
  ]
}
