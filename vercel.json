{
  "builds": [
    {
      "src": "nuxt.config.js",
      "use": "@nuxtjs/vercel-builder",
      "config": {
        "serverFiles": ["serverMiddleware/**", "_JWTKeys/**"]
      }
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/accounts/(.*)",
      "dest": "/api/accounts/$1.js",
      "methods": ["GET", "POST", "PUT", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Methods": "GET, POST, PUT, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Cookie"
      }
    },
    {
      "src": "/api/categories/(.*)",
      "dest": "/api/categories/$1.js",
      "methods": ["GET", "POST", "PUT", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Methods": "GET, POST, PUT, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Cookie"
      }
    },
    {
      "src": "/api/orders/(.*)",
      "dest": "/api/orders/$1.js",
      "methods": ["GET", "POST", "PUT", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Methods": "GET, POST, PUT, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Cookie"
      }
    },
    {
      "src": "/api/posts/(.*)",
      "dest": "/api/posts/$1.js",
      "methods": ["GET", "POST", "PUT", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Methods": "GET, POST, PUT, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Cookie"
      }
    },
    {
      "src": "/api/storedata/(.*)",
      "dest": "/api/storedata/$1.js",
      "methods": ["GET", "POST", "PUT", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Methods": "GET, POST, PUT, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Cookie"
      }
    },
    {
      "src": "/api/stripeTransactions00/(.*)",
      "dest": "/api/stripeTransactions00/$1.js",
      "methods": ["GET", "POST", "PUT", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Methods": "GET, POST, PUT, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Cookie"
      }
    },
    {
      "src": "/api/users/(.*)",
      "dest": "/api/users/$1.js",
      "methods": ["GET", "POST", "PUT", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Methods": "GET, POST, PUT, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Cookie"
      }
    },
    {
      "src": "/api/login/(.*)",
      "dest": "/api/login/$1.js",
      "methods": ["POST"],
      "headers": {
        "Access-Control-Allow-Methods": "POST",
        "Access-Control-Allow-Headers": "Content-Type, Cookie"
      }
    },
    {
      "src": "/api/me/(.*)",
      "dest": "/api/me/$1.js",
      "methods": ["GET"],
      "headers": {
        "Access-Control-Allow-Methods": "GET",
        "Access-Control-Allow-Headers": "Content-Type, Cookie"
      }
    }
  ]
}
